{
  "name": "unplugin-combine",
  "version": "0.9.0",
  "packageManager": "pnpm@8.15.6",
  "description": "Unified plugin that combines other plugins into one plugin.",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-combine#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-combine/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-combine.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "esbuild": ">=0.13",
    "rollup": "^3.2.0 || ^4.0.0",
    "vite": "^2.3.0 || ^3.0.0 || ^4.0.0 || ^5.0.0",
    "webpack": "4 || 5"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "^0.7.7",
    "unplugin": "^1.10.1"
  },
  "devDependencies": {
    "@rspack/core": "^0.5.9",
    "@sxzz/eslint-config": "^3.9.1",
    "@sxzz/prettier-config": "^2.0.1",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "esbuild": "^0.20.2",
    "eslint": "^9.0.0",
    "fast-glob": "^3.3.2",
    "prettier": "^3.2.5",
    "rolldown": "nightly",
    "rollup": "^4.14.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vitest": "^1.4.0",
    "webpack": "^5.91.0"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@sxzz/prettier-config"
}
