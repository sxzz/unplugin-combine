{
  "name": "unplugin-combine",
  "version": "0.6.0",
  "packageManager": "pnpm@8.6.4",
  "description": "Unified plugin that combines other plugins into one plugin.",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-combine#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-combine/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-combine.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "esbuild": ">=0.13",
    "rollup": "^3.2.0",
    "vite": "^2.3.0 || ^3.0.0 || ^4.0.0",
    "webpack": "4 || 5"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "^0.7.4",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@sxzz/eslint-config-prettier": "^2.4.6",
    "@sxzz/eslint-config-ts": "^2.4.6",
    "@types/node": "^18.16.18",
    "bumpp": "^9.1.1",
    "esbuild": "^0.18.8",
    "eslint": "^8.43.0",
    "eslint-define-config": "^1.21.0",
    "fast-glob": "^3.2.12",
    "prettier": "^2.8.8",
    "rollup": "^3.25.2",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.2",
    "webpack": "^5.88.0"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
